    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "prisma": "^3.6.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "bcrypt": "^5.0.1",
